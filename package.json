{
  "name": "xljsx-lite",
  "version": "1.0.0",
  "description": "Build .xlsx files in Javascript",
  "main": "lib/legacy.js",
  "module": "lib/module.js",
  "standalone-build": "lib/standalone.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "lint": "eslint . ./",
    "ci": "npm run test && npm run lint",
    "build": "npm run ci && rimraf lib/* && rollup -c"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      ".js": "./node_modules/babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/elliotstoner/xljsx-lite.git"
  },
  "keywords": [
    "JSON",
    "Excel",
    "xlsx",
    "JavaScript"
  ],
  "author": "Elliot Stoner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliotstoner/xljsx-lite/issues"
  },
  "homepage": "https://github.com/elliotstoner/xljsx-lite#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2",
    "lodash.escape": "^4.0.1"
  }
}
